/**********子界面背景**********/
QWidget#customWidget {
        background: rgb(173, 202, 232);
}

/**********子界面中央背景**********/
QWidget#centralWidget {
        border: 2px solid rgb(111, 156, 207);
        border-top: none;
        background: rgb(187, 212, 238);
}

/**********标题栏背景**********/
QWidget#titleBarContainer {
        border: none;
        background: rgb(170, 200, 230);
}

/**********主界面样式**********/
QWidget#mainWindow {
        border: 2px solid rgb(111, 156, 207);
        background: rgb(232, 241, 252);
}

QWidget#messageWidget {
        background: rgba(173, 202, 232, 50%);
}

QWidget#loadingWidget {
        border: none;
        border-radius: 5px;
        background: rgb(187, 212, 238);
}

QWidget#remoteWidget {
        border-top-right-radius: 10px;
        border-bottom-right-radius: 10px;
        border: 1px solid rgb(111, 156, 207);
        border-left: none;
        background: transparent;
}

TitleBarWidget {
    border: 2px solid rgb(111, 156, 207);
    background: rgb(187, 212, 238);
}

StyledWidget {
        qproperty-normalColor: rgb(65, 65, 65);
        qproperty-disableColor: rgb(180, 180, 180);
        qproperty-highlightColor: rgb(0, 160, 230);
        qproperty-errorColor: red;
}

QProgressIndicator {
        qproperty-color: rgb(2, 65, 132);
}

/*********** 测试项目杂集 ***********/
QTreeWidget#twMiscellaneousTesting
{
    border: 1px solid rgb(111, 156, 207);
    indentation: 0px;
}

QTreeWidget#twMiscellaneousTesting QHeaderView::section:horizontal
{
    border: none;
    show-decoration-selected: 1;
    border-bottom: 1px solid rgb(139,185,232);
}

QTreeWidget#twMiscellaneousTesting::item
{
    color: rgb(3,65,151);
    background-color: rgb(187,212,238);
    border-bottom: 1px solid rgb(111,156,207);
}

QTreeWidget#twMiscellaneousTesting::item:hover
{
    background-color: rgb(137,216,250);
}

QTreeWidget#twMiscellaneousTesting::item:selected
{
    background-color: rgb(137,216,250);
}

QTreeWidget#twMiscellaneousTesting::item:has-children
{
    background-color: rgb(120,170,220);
}

/**********提示**********/
QToolTip{
        border: 1px solid rgb(111, 156, 207);
        background: white;
        color: rgb(51, 51, 51);
}

/**********菜单栏**********/
QMenuBar {
        background: rgb(187, 212, 238);
        border: 1px solid rgb(111, 156, 207);
}
QMenuBar::item {
        border: 1px solid transparent;
        padding: 5px 10px 5px 10px;
        background: transparent;
}
QMenuBar::item:enabled {
        color: rgb(2, 65, 132);
}
QMenuBar::item:!enabled {
        color: rgb(155, 155, 155);
}
QMenuBar::item:enabled:selected {
        border-top-color: rgb(111, 156, 207);
        border-bottom-color: rgb(111, 156, 207);
        background: rgb(198, 224, 252);
}

/**********菜单**********/
QMenu {
        border: 1px solid rgb(111, 156, 207);
        background: rgb(232, 241, 250);
}
QMenu::item {
        height: 22px;
        padding: 0px 25px 0px 20px;
}
QMenu::item:enabled {
        color: rgb(84, 84, 84);
}
QMenu::item:!enabled {
        color: rgb(155, 155, 155);
}
QMenu::item:enabled:selected {
        color: rgb(2, 65, 132);
        background: rgba(255, 255, 255, 200);
}
QMenu::separator {
        height: 1px;
        background: rgb(111, 156, 207);
}
QMenu::indicator {
        width: 13px;
        height: 13px;
}
QMenu::icon {
        padding-left: 2px;
        padding-right: 2px;
}

/**********状态栏**********/
QStatusBar {
        background: rgb(187, 212, 238);
        border: 1px solid rgb(111, 156, 207);
        border-top: none;
}
QStatusBar::item {
    border: none;
    border-right: 1px solid rgb(111, 156, 207);
}

/**********分组框**********/
QGroupBox {
        font-size: 15px;
        border: 1px solid rgb(111, 156, 207);
        border-radius: 4px;
        margin-top: 10px;
}
QGroupBox::title {
        color: rgb(56, 99, 154);
        top: -12px;
        left: 10px;
}

/**********工具栏**********/
QToolBar {
        border: none;
        background: rgb(170, 200, 230);
        padding: 5px;
}

/**********标题栏**********/
QToolBar#winTitleBar {
        border: 2px solid rgb(111, 156, 207);
        border-bottom: none;
        background: rgb(187, 212, 238);
        padding: 5px;
}

/**********页签项**********/
QTabWidget::pane {
        border: none;
        border-top: 3px solid rgb(0, 78, 161);
        background: rgb(187, 212, 238);
}
QTabWidget::tab-bar {
        border: none;
}

QTabWidget#mainTabWidget::pane {
        border-top: 3px solid rgb(0, 78, 161, 200);
}

QTabWidget#tabWidget::pane {
        border: 1px solid rgb(111, 156, 207);
        background: rgb(232, 241, 252);
        margin-top: -1px;
}

QTabBar::tab {
        border: none;
        border-top-left-radius: 4px;
        border-top-right-radius: 4px;
        color: white;
        background: rgb(120, 170, 220);
        height: 28px;
        min-width: 85px;
        margin-right: 5px;
        padding-left: 5px;
        padding-right: 5px;
}
QTabBar::tab:hover {
        background: rgb(0, 78, 161);
}
QTabBar::tab:selected {
        color: white;
        background: rgb(0, 78, 161);
}

QTabBar#mainTabWidget_TabBar::tab {
        border: none;
        border-top-left-radius: 0px;
        border-top-right-radius: 0px;
        color: white;
        background: rgb(120, 170, 220);
        height: 50px;
        min-width: 70px;
        margin-right: 1px;
        padding-left: 5px;
        padding-right: 5px;
}
QTabBar#mainTabWidget_TabBar::tab:hover {
        background: rgb(0, 78, 161, 150);
}
QTabBar#mainTabWidget_TabBar::tab:selected {
        color: white;
        background: rgb(0, 78, 161, 200);
}

QTabBar#tabBar::tab {
        border: 1px solid rgb(111, 156, 207);
        border-bottom: none;
        color: rgb(70, 71, 73);
        background: transparent;
}
QTabBar#tabBar::tab:hover {
        color: rgb(2, 65, 132);
}
QTabBar#tabBar::tab:selected {
        color: rgb(2, 65, 132);
        background: rgb(232, 241, 252);
}

/**********分裂器**********/

QSplitter {
    width: 3px;
}

QSplitter::handle {
    background: rgb(0, 78, 161);
}

/**********表头**********/
QHeaderView {
        border: none;
        border-right: none;
        background: transparent;
        min-height: 30px;
}
QHeaderView::section:horizontal {
        border: 1px solid rgb(111, 156, 207);
        border-left: none;
        color: rgb(2, 65, 132);
        background: transparent;
        padding-left: 5px;
}
QHeaderView::section:horizontal:first {
        border-left: 1px solid rgb(111, 156, 207);
}
QHeaderView::section:horizontal:hover {
        color: white;
        background: rgb(0, 78, 161);
}
QHeaderView::section:horizontal:pressed {
        color: white;
        background: rgb(6, 94, 187);
}
QHeaderView::up-arrow {
        width: 13px;
        height: 11px;
        padding-right: 5px;
        image: url(:/White/topArrow);
        subcontrol-position: center right;
}
QHeaderView::up-arrow:hover, QHeaderView::up-arrow:pressed {
        image: url(:/White/topArrowHover);
}
QHeaderView::down-arrow {
        width: 13px;
        height: 11px;
        padding-right: 5px;
        image: url(:/White/bottomArrow);
        subcontrol-position: center right;
}
QHeaderView::down-arrow:hover, QHeaderView::down-arrow:pressed {
        image: url(:/White/bottomArrowHover);
}

/**********表格**********/
QTableView {
        border: none;/*1px solid rgb(111, 156, 207);*/
        background: rgb(224, 238, 255);
        gridline-color: rgb(111, 156, 207);
}
QTableView::item {
        padding-left: 3px;
