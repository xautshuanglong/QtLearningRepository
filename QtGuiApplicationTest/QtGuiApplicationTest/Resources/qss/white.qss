/**********滚动条-水平**********/
QScrollBar:horizontal {
        height: 18px;
        background: transparent;
        margin-top: 1px;
        margin-bottom: 1px;
        padding-left: 18px;
        padding-right: 18px;
}
QScrollBar::handle:horizontal {
        height: 10px;
        min-width: 30px;
        background: rgb(165, 195, 225);
        margin-top: 3px;
        margin-bottom: 3px;
        border-radius:5px;
}
QScrollBar::handle:horizontal:hover {
        background: rgb(107,173,232);
}
QScrollBar::sub-line:horizontal {
        width: 18px;
        background: transparent;
        image: url(:/AppImages/Resources/images/white/arrow_left.png);
        subcontrol-position: left;
}
QScrollBar::add-line:horizontal {
        width: 18px;
        background: transparent;
        image: url(:/AppImages/Resources/images/white/arrow_right.png);
        subcontrol-position: right;
}
QScrollBar::sub-line:horizontal:hover {
        /*background: rgb(113,186,245);*/
}
QScrollBar::add-line:horizontal:hover {
        /*background: rgb(113,186,245);*/
}
QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal {
        background: transparent;
}

/**********滚动条-垂直**********/
QScrollBar:vertical {
        width: 18px;
        background: transparent;
        margin-left: 1px;
        margin-right: 1px;
        padding-top: 18px;
        padding-bottom: 18px;
}
QScrollBar::handle:vertical {
        width: 10px;
        min-height: 18px;
        background: rgb(165, 195, 225);
        margin-left: 3px;
        margin-right: 3px;
        border-radius:5px;
}
QScrollBar::handle:vertical:hover {
        background: rgb(107,173,232);
}
QScrollBar::sub-line:vertical {
        height: 18px;
        background: transparent;
        image: url(:/AppImages/Resources/images/white/arrow_up.png);
        subcontrol-position: top;
}
QScrollBar::add-line:vertical {
        height: 18px;
        background: transparent;
        image: url(:/AppImages/Resources/images/white/arrow_down.png);
        subcontrol-position: bottom;
}
QScrollBar::sub-line:vertical:hover {
        /*background: rgb(113,186,245);*/
}
QScrollBar::add-line:vertical:hover {
        /*background: rgb(113,186,245);*/
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
        background: transparent;
}

QScrollBar#verticalScrollBar:vertical {
        margin-top: 30px;
}

/**********下拉列表**********/
QComboBox {
        height: 25px;
        border-radius: 4px;
        border: 1px solid rgb(111, 156, 207);
        background: white;
}
QComboBox[lightGreenFlag='true'] {
        background-color: lightgreen;
}
QComboBox[lightGreenFlag='false'] {
        background-color: lightblue;
}
QComboBox:enabled {
        color: rgb(84, 84, 84);
}
QComboBox:!enabled {
        color: rgb(80, 80, 80);
}
QComboBox:enabled:hover, QComboBox:enabled:focus {
        color: rgb(51, 51, 51);
}
QComboBox::drop-down {
        width: 20px;
        border: none;
        background: transparent;
}
QComboBox::drop-down:hover {
        background: rgba(255, 255, 255, 30);
}
QComboBox::down-arrow
{
       image: url(:/AppImages/Resources/images/white/arrow_down.png);
}
QComboBox::down-arrow:on {
        /**top: 1px;**/
       image: url(:/AppImages/Resources/images/white/arrow_up.png);
}
QComboBox QAbstractItemView {
        border: 1px solid rgb(111, 156, 207);
        background-color: white;
        border: 2px solid green;
        border-radius: 10px;
}
QComboBox QAbstractItemView::item {
        height: 30px;
        color: rgb(73, 73, 73);
        background-color: gray;
        border-radius: 10px;
        outline: none;
}
QComboBox QAbstractItemView::item:selected {
        background: rgb(111, 156, 207);
        color: white;
}

/**********进度条**********/
QProgressBar{
        border: none;
        text-align: center;
        color: white;
        background: rgb(173, 202, 232);
}
QProgressBar::chunk {
        background: rgb(16, 135, 209);
}

QProgressBar#progressBar {
        border: none;
        text-align: center;
        color: white;
        background-color: transparent;
        background-image: url(":/White/progressBar");
        background-repeat: repeat-x;
}
QProgressBar#progressBar::chunk {
        border: none;
        background-color: transparent;
        background-image: url(":/White/progressBarChunk");
        background-repeat: repeat-x;
}

